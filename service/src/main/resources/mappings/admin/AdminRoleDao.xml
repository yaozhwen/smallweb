<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.service.dao.admin.AdminRoleDao">

    <select id="get" resultType="AdminRole">
        SELECT
		a.*
		FROM ADMIN_ROLE a
		WHERE a.ID = #{id}
    </select>

</mapper>